<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="253.0" prefWidth="421.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectoagenciamejorado.Controllers.ActualizarClienteController">
   <children>
      <AnchorPane prefHeight="50.0" prefWidth="421.0" style="-fx-background-color: #494858;">
         <children>
            <Label layoutX="159.0" layoutY="18.0" text="Actualizar Cliente" textFill="WHITE">
               <font>
                  <Font name="Georgia" size="13.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="45.0" layoutY="75.0" text="Identificación:">
         <font>
            <Font name="Georgia" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="identificacionTextField" layoutX="211.0" layoutY="70.0" promptText="Ingrese identificación" />
      <Button layoutX="173.0" layoutY="104.0" mnemonicParsing="false" text="Buscar" onAction="#handleBuscarCliente">
         <font>
            <Font name="Georgia" size="13.0" />
         </font>
      </Button>
      <Label layoutX="69.0" layoutY="151.0" text="Nombre:">
         <font>
            <Font name="Georgia" size="13.0" />
         </font>
      </Label>
      <Label layoutX="45.0" layoutY="185.0" text="Correo Electrónico:">
         <font>
            <Font name="Georgia" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="nombreTextField" layoutX="211.0" layoutY="139.0" promptText="Ingrese su nombre" />
      <TextField fx:id="correoElectronicoTextField" layoutX="211.0" layoutY="180.0" promptText="Ingrese su correo" />
      <Button layoutX="76.0" layoutY="214.0" mnemonicParsing="false" text="Actualizar" onAction="#handleActualizarCliente">
         <font>
            <Font name="Georgia" size="13.0" />
         </font>
      </Button>
      <Button layoutX="252.0" layoutY="214.0" mnemonicParsing="false" text="Cancelar" onAction="#handleCancelar">
         <font>
            <Font name="Georgia" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
